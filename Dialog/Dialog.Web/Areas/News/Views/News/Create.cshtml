@model CreateViewModel

@{
    var partialModel = new JumbotronPartialViewModel
    {
        Title = "Create News",
        Current = "Create",
        Parent = "News",
        ImageUri = "https://res.cloudinary.com/aguzelov/image/upload/v1544704896/rdc9ezgkeratzej3otlx.jpg"
    };

}

<partial name="~/Views/Shared/_JumbotronPartial.cshtml" model=@partialModel />
<section class="ftco-section">
    <div class="container">
        <form asp-area="News" asp-controller="News" asp-action="Create" enctype="multipart/form-data" method="post">
            <label asp-for="@Model.Title"></label>
            <input asp-for="@Model.Title" />
            <br />
            <label asp-for="@Model.Content"></label>
            <textarea rows="10" asp-for="@Model.Content"></textarea>
            <br />
            <input asp-for="@Model.UploadImages" multiple />
            <br />
            <input class="btn btn-primary py-3 px-5" type="submit" />
        </form>
    </div>
</section>

<script src="https://cloud.tinymce.com/stable/tinymce.min.js"></script>
<script>tinymce.init({ selector: 'textarea' });</script>